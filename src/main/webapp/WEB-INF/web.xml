<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
    <servlet>
        <servlet-name>LoginServlet</servlet-name>
        <servlet-class>com.var.agregator.servlets.LoginServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>LoginServlet</servlet-name>
        <url-pattern>/Login</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>RegistrationServlet</servlet-name>
        <servlet-class>com.var.agregator.servlets.RegistrationServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>RegistrationServlet</servlet-name>
        <url-pattern>/Registration</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>LogoutServlet</servlet-name>
        <servlet-class>com.var.agregator.servlets.LogOutServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>LogoutServlet</servlet-name>
        <url-pattern>/Logout</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>InitDataServlet</servlet-name>
        <servlet-class>com.var.agregator.servlets.InitDataServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>InitDataServlet</servlet-name>
        <url-pattern>/InitData</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>CrteateTripPreferencesServlet</servlet-name>
        <servlet-class>com.var.agregator.servlets.CreateTripPreferences</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>CrteateTripPreferencesServlet</servlet-name>
        <url-pattern>/CreateTripPreferences</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>ViewTripPreferencesServlet</servlet-name>
        <servlet-class>com.var.agregator.servlets.ViewTripPreferencesServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ViewTripPreferencesServlet</servlet-name>
        <url-pattern>/ViewTripPreferences</url-pattern>
    </servlet-mapping>




</web-app>
